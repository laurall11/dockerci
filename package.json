{
    "name": "dockerci",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3001",
        "build": "next build",
        "start": "next start",
        "test": "jest",
        "lint": "eslint -o Outputfile -f tap .",
        "format": "prettier-eslint --eslint-config-path ./.eslintrc.js --write '**/*.js'",
        "ci": "jest && npm run lint && npm run format",
        "ci-rebuild": "jest && npm run lint && docker-compose up --build"
    },
    "dependencies": {
        "@types/node": "^14.11.10",
        "@types/react": "^16.9.53",
        "next": "9.5.5",
        "react": "16.14.0",
        "react-dom": "16.14.0",
        "typescript": "^4.0.3"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "jest": "^26.6.0",
        "prettier": "^2.1.2",
        "prettier-eslint": "^11.0.0",
        "prettier-eslint-cli": "^5.0.0"
    }
}